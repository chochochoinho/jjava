		// TODO Auto-generated method stub

		int a=0; 
		int b=0;
		int c=0;
	
		
Scanner sc= new Scanner(System.in);





System.out.println("ют╥б");

//int x=sc.nextInt();
int ax=1;

int xx=sc.nextInt();
	for(a=xx; ax<=9; ax++) {
		
		
	//	System.out.println("+"a"+ * +"ax+" = "+(a*ax));
		System.out.printf("%d * %d = %d\n", a,ax,(a*ax));
		
		
