		 //int p=60; java=70; , android =80; , c=50;,cplus=30;
		 int p=60;
		 int java=70;
		 int android=80;
		 int cplus =30;
		 int c=50; 
		 
		 
		 float rvg; 
		 int sum=0;
		 
		 sum=p+java+android+cplus+c;
		 rvg = sum / 5;
		 rvg =(float)(sum) / 5; 
		 
		
	//	 System.out.printf("%d", sum+\n+);
		 //printf " 데이터형  " ,매개변수 )   
		 
